{
  "name": "dinesh-pinetco/excel",
  "description": "Supercharged Excel exports and imports in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "phpspreadsheet",
    "phpexcel",
    "excel",
    "csv",
    "export",
    "import",
    "batch"
  ],
  "authors": [
    {
      "name": "Patrick Brouwers",
      "email": "patrick@spartner.nl"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": "^7.0|^8.1",
    "phpoffice/phpspreadsheet": "^1.18",
    "illuminate/support": "5.8.*|^6.0|^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0|^7.0",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "dinesh-pinetco\\Excel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "dinesh-pinetco\\Excel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "dinesh-pinetco\\Excel\\ExcelServiceProvider"
      ],
      "aliases": {
        "Excel": "dinesh-pinetco\\Excel\\Facades\\Excel"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
